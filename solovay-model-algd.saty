let enum-format enumeratef =
  let labelf ctx idx =
    let fsizer r = get-font-size ctx *' r in
    (enumeratef ctx idx |> EnumitemFormatUtil.align-right (fsizer 1.5))
      ++ inline-skip (fsizer 0.5)
  in
  EnumitemFormat.new-ordered labelf
